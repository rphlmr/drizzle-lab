    "react-router-hono-server": "^2.21.0",
    "remix-hono": "^0.0.18",
    "remix-utils": "^8.8.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "tw-animate-css": "catalog:",
    "use-debounce": "^10.0.3",
    "valibot": "^1.0.0-beta.14",
    "xstate": "^5.20.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@react-router/dev": "^7.8.0",
    "@shikijs/monaco": "^3.9.2",
    "@sly-cli/sly": "^2.1.1",
    "@tailwindcss/vite": "catalog:",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "glob": "^11.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
