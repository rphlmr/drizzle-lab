  "scripts": {
    "dev:watch": "tsup --config tsup.config.ts --watch",
    "build": "tsup --config tsup.config.ts",
    "postbuild": "pnpm run check-exports",
    "check-exports": "attw --pack .",
    "typecheck": "tsc --noEmit",
    "prerelease:publish": "pnpm version prerelease && pnpm publish --access=public --tag next",
    "patch:publish": "pnpm version patch && pnpm publish --access=public",
    "minor:publish": "pnpm version minor && pnpm publish --access=public",
    "check": "biome check .",
    "check:fix": "biome check --fix ."
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
